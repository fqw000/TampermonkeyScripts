// ==UserScript==
// @name         Bypass DevTools Detection
// @namespace    http://tampermonkey.net/
// @version      1.0
// @description  绕过开发者工具检测
// @author       Zephyr
// @match        https://*.*
// @grant        none
// ==/UserScript==

(function() {
    'use strict';
    window.debuggerFC = function() { console.log('debugger blocked'); };

    // 屏蔽控制台检测
    console.log = function() {};

    // 阻止页面跳转
    window.addEventListener('beforeunload', function(e) {
        e.preventDefault();
        e.returnValue = '';
        return '';
    });

    // 重写location方法
    Object.defineProperty(window, 'location', {
        value: {
            href: window.location.href,
            replace: function() {},
            assign: function() {},
            reload: function() {}
        },
        writable: false
    });
})();
